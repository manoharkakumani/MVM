class Animal():
    fn __init__(self,name):
        self.name = name
    fn eat(self):
        print(self.name,"can eat")

class Dog(Animal):
    fn __init__(self,name):
        self.name = name
    fn bark(self):
        print(self.name,"can bark")

a= 'Tom' |> Dog()
a.bark()
a.eat()
